{
  "name": "rookbot",
  "version": "1.5.0",
  "description": "Discord bot for personal usage",
  "main": "src/index.js",
  "scripts": {
    "audit": "node ./src/res/ci/common/run-audit.js",
    "local-install": "node ./src/res/ci/common/run-install.js",
    "menu": "node ./src/res/ci/common/menu.js",
    "run": "node ./run.js",
    "update": "node ./src/res/ci/common/run-update.js",
    "ver": "node ./src/res/ci/common/ver.js",
    "castle-dev": "bash ./node_modules/.bin/dotenvx run -f ./env/devs/.env.token.castle -f ./env/devs/.env.client.castle -f ./env/envs/.env.dev -- npm run-script run",
    "minnie-dev": "bash ./node_modules/.bin/dotenvx run -f ./env/devs/.env.token.minnie -f ./env/devs/.env.client.minnie -f ./env/envs/.env.dev -- npm run-script run",
