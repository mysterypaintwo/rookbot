    "minnie-prod": "bash ./node_modules/.bin/dotenvx run -f ./env/devs/.env.token.minnie -f ./env/devs/.env.client.minnie -f ./env/envs/.env.prod -- npm run-script run",
    "nikose-prod": "bash ./node_modules/.bin/dotenvx run -f ./env/devs/.env.token.nikose -f ./env/devs/.env.client.nikose -f ./env/envs/.env.prod -- npm run-script run",
