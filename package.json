    "discord.js": "^14.16.3",
    "dotenv": "^16.4.5",
    "mathjs": "^13.2.2",
    "ms": "^2.1.3",
    "node-schedule": "^2.1.1",
    "pagination.djs": "^4.0.17",
    "pm2": "^5.4.3",
    "semver": "^7.6.3",
    "shelljs": "^0.8.5",
    "timezone": "^1.0.23"
  }
